{
  "name": "@hh.ru/eslint-plugin-import-rules",
  "version": "1.0.0",
  "main": "lib/index.js",
  "description": "HH.ru eslint import rules",
  "scripts": {
    "build": "babel --ignore '**/*.test.js' --quiet --out-dir lib src",
